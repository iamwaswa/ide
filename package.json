{
  "name": "IDE",
  "private": true,
  "description": "An educational IDE platform",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Waswa Olunga <olungaw@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --open --port 8080",
    "serve": "gatsby serve",
    "test": "echo 'Write tests! -> https://gatsby.dev/unit-testing'",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint --fix",
    "format": "npm run prettier && npm run lint:fix"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.10",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "firebase": "^7.14.2",
    "gatsby": "^2.20.20",
    "gatsby-image": "^2.3.2",
    "gatsby-plugin-config": "^1.0.7",
    "gatsby-plugin-create-client-paths": "^2.2.3",
    "gatsby-plugin-firebase": "^0.2.0-beta.4",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-transition-link": "^1.18.0",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-source-sanity": "^5.0.6",
    "gatsby-theme-material-ui": "^1.0.8",
    "gatsby-transformer-sharp": "^2.4.4",
    "gsap": "^3.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/material-ui": "^0.21.7",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run format",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamwaswa/ide.git"
  },
  "bugs": {
    "url": "https://github.com/gatsbyts/gatsby/issues"
  }
}
